<% if current_user.profile.liked_posts.exclude?(post) %>
  <%= form_with model: Like.new do |f| %>
    <%= f.hidden_field :liked_post_id, value: post.id  %>
    <%= f.submit 'like' %>
  <% end %>
<% else %>
  <%= form_with model: like, method: :delete do |f| %>
    <%= f.submit 'un-like' %>
  <% end %>
<% end %>
<br>